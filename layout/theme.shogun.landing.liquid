<script>
      $(document).ready(function() {

        function initializeSlider(containerClass, sliderOptions) {
          $(containerClass).slick(sliderOptions);
        }

        function initSectionSlider(sectionId) {
          //  alert('Initializing slider for section: ' + sectionId);

          if (sectionId === 'new-product-template-product-details' || sectionId === 'new-family-package-product-details' || sectionId === 'new-patriclan-template-product-details') {
            initializeSlider('.banner-product-carousel', {
              infinite: true,
              slidesToShow: 1,
              slidesToScroll: 1,
              dots: true,
              arrows: false,
              centerMode: true,
              centerPadding: '20'
              
            });
          } else if (sectionId === 'new-product-template-how-it-works' || sectionId === 'new-family-package-how-it-works' || sectionId === 'new-patriclan-template-how-it-works') {
            initializeSlider('.how-it-works-corousel', {
              dots: false,
              infinite: true,
              arrows: true,
              speed: 300,
              slidesToShow: 1,
              centerMode: true,
              variableWidth: false,
              responsive: [
                {
                  breakpoint: 1024,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1
                  }
                },
                {
                  breakpoint: 600,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,  
                    variableWidth: false,
                    centerPadding: '20'
                  }
                },
                {
                  breakpoint: 480,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,                  
                    variableWidth: false,
                    centerPadding: '20'
                  }
                }
              ]
            });
          }
          else if (sectionId === 'new-product-template-what-you-get' || sectionId === 'new-family-package-what-you-get' || sectionId === 'new-patriclan-template-what-you-get') {
            initializeSlider('.what_you-get_image_container', {
              dots: true,
              infinite: false,
              speed: 300,
              slidesToShow: 1,
              slidesToScroll: 1,
              responsive: [
                {
                  breakpoint: 1024,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    infinite: true,
                    dots: true
                  }
                },
                {
                  breakpoint: 600,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1
                  }
                },
                {
                  breakpoint: 480,
                  settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1
                  }
                }
              ]
            });
          }
          // Add more conditions for other sections if needed
        }

        $(document).on('shopify:section:load', function(event) {
          var sectionId = event.detail.sectionId;
          initSectionSlider(sectionId);
        });

        // Initialize sliders for the first load
          // Initialize for new-product-template
          initSectionSlider('new-product-template-product-details');
          initSectionSlider('new-product-template-how-it-works');
          initSectionSlider('new-product-template-what-you-get');

          // Initialize for new-family-package-template
          initSectionSlider('new-family-package-product-details');
          initSectionSlider('new-family-package-how-it-works');
          initSectionSlider('new-family-package-what-you-get');

          // Initialize for new-patriclan-template
          initSectionSlider('new-patriclan-template-product-details');
          initSectionSlider('new-patriclan-template-how-it-works');
          initSectionSlider('new-patriclan-template-what-you-get');
      })
    </script>
